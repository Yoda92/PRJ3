/dts-v1/;
/plugin/;
/ {
  compatible = "brcm,bcm2835", "brcm,bcm2836", "brcm,bcm2708", "brcm,bcm2709";
  fragment@0 {

    target-path = "/";
    __overlay__ {

      driverLCD: driverLCD@0 {

        compatible = "PRJ3, driverLCD";

        gpios = <&gpio 20 1>, <&gpio 21 1>, <&gpio 22 1>, <&gpio 23 1>, <&gpio 24 1>, <&gpio 25 1>, <&gpio 26 1>, <&gpio 27 1>, <&gpio 19 1>, <&gpio 16 1>, <&gpio 13 1>;
        status = "okay";


      };
    };
  };
};
